page{
    width: 100%;
    min-height: 100%;
    background-color: #f2f2f2;
}
.container{
     min-height: 100%;
}
.container-kong{
    margin: 320rpx auto 0 auto;
    width: 256rpx;
    height: 240rpx;
}
.container-kong image{
    width: 256rpx;
    height: 240rpx;
}
.coupon-none{
    text-align: center;
    margin-top: 30rpx;
    font-size: 30rpx;
    color: #f08d7e;
}
.coupon-item{
    width: 710rpx;
    margin: 30rpx auto 0 auto;
    height: 200rpx;
    background-image: url("data:image/png;base64,");
    box-shadow: 0 4rpx 6rpx 0 rgba(0,0,0,0.09);
    border-radius: 8rpx;
    background-repeat: no-repeat;
    background-size: cover;
}

.coupon-item-top{
    padding: 60rpx 30rpx 20rpx 30rpx;
    display: flex;
    flex-flow: row nowrap;
    justify-content: space-between;
}
.price{
    opacity: 0.72;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #D53333;
}
.use-btn{
    width: 152rpx;
    height: 54rpx;
    line-height: 54rpx;
    text-align: center;
    color: #fff;
    background: #D53333;
    border-radius: 4rpx;
    font-size: 28rpx;
}
.coupon-time{
    font-size: 26rpx;
    color: #AAAAAA;
    padding-left: 30rpx;
}
.guoqi{
    opacity: 0.72;
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: #AAAAAA;
}
.guoqi-right{
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #BBBBBB;
}