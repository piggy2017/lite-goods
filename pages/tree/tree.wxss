}
.prompt{
    position: absolute;
    z-index: 9;
    left: 530rpx;
    width: 100rpx;
    height: 50rpx;
    opacity: 0;
    top: 100rpx;
    color: #FAA520;
}
.golddown{
    animation: down 1.6s;
}
.golddown-2{
    animation: down2 1.8s;
}
.golddown-3{
    animation: down3 1.8s;
}
.faded{
    animation: fadeNum 1.8s;
}
@keyframes down{
    0%{
        opacity: 1;
        top:30rpx;
        z-index:9;
    }
    100% {
        opacity: 0;
        top: 460rpx;
        z-index:9;
    }
}
@keyframes down2{
    0%{
        opacity: 1;
        top:80rpx;
        z-index:9;
    }
    100% {
        opacity: 0;
        top: 460rpx;
        z-index:9;
    }
}
@keyframes down3{
    0%{
        opacity: 1;
        top:100rpx;
        z-index:9;
    }
    100% {
        opacity: 0;
        top: 460rpx;
        z-index:9;
    }
}
@keyframes fadeNum{
    0%{
        opacity: 0;
    }
    30%{
        opacity: 1;
    }
    100% {
        opacity: 0;
    }
}
.hidden{
    display: none !important;
}
.rule-mode{
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background-color: rgba(0,0,0,0.7);
    z-index: 99;
    display: flex;
    flex-flow: row nowrap;
    align-items: center;
    justify-content: center;
}
.rule-wraper{
    width: 600rpx;
    min-height: 430rpx;
    background: #FFFFFF;
    border-radius: 6px;
    position: relative;
}
.rules-content{
    width: 600rpx;
    min-height: 430rpx;
