page{
    width: 100%;
    min-height: 100%;
    background-size: cover;
    background-repeat: no-repeat;
    font-family: PingFangSC-Regular;
    background-position: center center;
    background-image: url("http://img.tracenet.cn/bg1.png");
}
.tree{
    width: 100%;
    min-height: 100%;
    position: relative;
}
.tree-top{
    width: 710rpx;
    height: 114rpx;
    margin: 30rpx auto 0 auto;
    display: flex;
    flex-flow: row nowrap;
}
.surplus-number,.continuous-sign{
    height: 100%;
    width: 210rpx;
    background: rgba(13, 74, 160, 0.5);
    border-radius: 4px;
    font-size: 30rpx;
}
.tree-top-title{
    margin-left: 16rpx;
    margin-top: 10rpx;
    letter-spacing: 0;
    text-align: justify;
    opacity: 0.8;
    color: #FFFFFF;
}
.surplus-number-view{
    margin-top: 4rpx;
    margin-left: 16rpx;
    display: flex;
    flex-flow: row nowrap;
    align-items: center;
}
.surplus-number-view-btn{
    width: 40rpx;
    height: 40rpx;
    line-height: 34rpx;
    padding: 0;
    margin-left: 8rpx;
    border-radius: 50%;
    background-color: #fff;
    font-size: 46rpx;
    font-weight: bold;
    color: #0D4AA0;
    text-align: center;
}
.surplus-number-view image{
    width: 40rpx;
    height: 40rpx;
    margin-left: 8rpx;
}
.text-green{
    font-family: PingFangSC-Semibold;
    font-size: 36rpx;
