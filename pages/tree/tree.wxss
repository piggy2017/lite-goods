    opacity: 0;
}
.position-img-gold-2{
    width: 80rpx;
    height: 80rpx;
    position: absolute;
    z-index: -1;
    left: 270rpx;
    top:80rpx;
    opacity: 0;
}
.position-img-gold-3{
    width: 85rpx;
    height: 85rpx;
    position: absolute;
    z-index: -1;
    left: 410rpx;
    top:110rpx;
    opacity: 0;
}
.position-img-gold-3 image{
    width: 85rpx;
    height: 85rpx;
}
.position-img-gold-2 image{
    width: 80rpx;
    height: 80rpx;
}
.position-img-gold image{
    width: 90rpx;
