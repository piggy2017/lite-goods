}

.fans-number-left {
    background: #d81d1a;
    padding: 4rpx 4rpx;
    border-bottom-left-radius: 4rpx;
    border-top-left-radius: 4rpx;
    position: relative;
}

.fans-number-left::after {
    content: "";
    z-index: 9;
    width: 0;
    height: 0;
    position: absolute;
    top: 0;
    right: -20rpx;
    border-left: 20rpx solid #d81d1a;
    border-top: 20rpx solid transparent;
    border-bottom: 20rpx solid transparent;
}

.fans-number-right {
    background: #ecb06b;
    padding: 4rpx 4rpx;
    border-bottom-right-radius: 4rpx;
    border-top-right-radius: 4rpx;
    position: relative;
}
.fans-number-right::after{
     content: "";
    z-index: 9;
    width: 0;
    height: 0;
    position: absolute;
    top: 0;
    left: -20rpx;
    border-right: 20rpx solid #ecb06b;
    border-top: 20rpx solid transparent;
    border-bottom: 20rpx solid transparent;
}
