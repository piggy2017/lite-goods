page{
    min-height: 100%;
    background-color: #f2f2f2;
}
.container{
     min-height: 100%;
}
.container-kong{
    margin: 320rpx auto 0 auto;
    width: 256rpx;
    height: 240rpx;
}
.container-kong image{
    width: 256rpx;
    height: 240rpx;
}
.guangguang{
    margin: 60rpx auto 0 auto;
    width: 304rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    border-radius: 40rpx;
    border: 1rpx solid #e6e6e6;
    color:#aaa;
    font-size: 28rpx;
}
.cart-bg{
    width: 100%;
    height: 140rpx;
    background: #D53333;
    border-bottom-left-radius: 16rpx;
    border-bottom-right-radius: 16rpx;
    font-family: PingFangSC-Medium;
    font-size: 30rpx;
    color: #FFFFFF;
    display: flex;
    flex-flow: row nowrap;
    justify-content: space-between;
    padding-top: 24rpx;
}
.cart-bg-num{
    margin-left: 40rpx;
}
.cart-bg-text{
    margin-right: 40rpx;
}
.cart-list{
    position: absolute;
    z-index: 99;
    width: 690rpx;
    left: 30rpx;
    top: 100rpx;
    margin-bottom: 200rpx; 
}
.cart-list-scroll{
    width: 100%;
    height: 860rpx;
    /* padding-bottom: 30rpx; */
}
.cart-item{
    width: 100%;
    height: 224rpx;
    background-color: #fff;
    border-radius: 8rpx;
    position: relative;
}
.cart-item:not(:first-child){
    margin-top: 24rpx;
}
.cart-item-url{
    width: 100%;
    height: 224rpx;
    display: flex;
    flex-flow: row nowrap;
    align-items: center;
    justify-content: center;
}
.cart-item-left{
    width: 184rpx;
    height: 184rpx;
    border-radius: 8rpx;
    position: relative;
}
.xiajia{
    width: 184rpx;
    height: 40rpx;
    text-align: center;
    line-height: 40rpx;
    color: #fff;
    position: absolute;
    bottom: 0;
    background-color: rgba(213,51,51,0.7);
    font-size: 20rpx;
    z-index: 9;
    border-bottom-left-radius: 8rpx;
    border-bottom-right-radius: 8rpx;
    font-family: PingFangSC-Medium;
}
.shouwan{
    width: 184rpx;
    height: 40rpx;
    text-align: center;
    line-height: 40rpx;
    color: #fff;
    position: absolute;
    bottom: 0;
    background-color: rgba(0,0,0,0.5);
    font-size: 20rpx;
    z-index: 9;
    font-family: PingFangSC-Medium;
    border-bottom-left-radius: 8rpx;
    border-bottom-right-radius: 8rpx;
}
.cart-item-left image{
    width: 184rpx;
    height: 184rpx;
    border-radius: 8rpx;
}
.cart-item-right{
    height: 184rpx;
    margin-left: 20rpx;
    width: 432rpx; 
    position: relative;
}
.cart-item-name{
    width: 432rpx;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical !important;
    overflow: hidden;
    font-family: PingFangSC-Medium;
    font-size: 30rpx;
    color: #333333;
    font-weight: bold;
}
.cart-item-specifications{
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #888888;
    margin-top: 10rpx;
    width: 432rpx;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical !important;
    overflow: hidden;
}
.cart-item-bot{
    width: 422rpx;
    display: flex;
    right: 40rpx;
    flex-flow: row nowrap;
    justify-content: space-between;
    bottom: 20rpx;
    position: absolute;
    z-index: 99;
}
.cart-item-price{
    font-family: PingFangSC-Medium;
    font-size: 28rpx;
    color: #D53333;
    font-weight: bold;
}
.cart-item-computed{
    display: flex;
    flex-flow: row nowrap;
    align-items: center;
    height: 40rpx;
}
.reduce-num,.add-num{
    height: 40rpx;
    display: flex;
    flex-flow: row nowrap;
    align-items: center;
}
.reduce-num image{
    width: 34rpx;
    height: 8rpx;
}
.add-num image{
    width: 34rpx;
    height: 34rpx;
}
.cart-item-number{
    margin: 0 16rpx;
    width: 40rpx;
    font-family: PingFangSC-Medium;
    font-size: 28rpx;
    color: #000000;
    height: 40rpx;
    line-height: 40rpx;
    background: #EAEAEA;
    border-radius: 4rpx;
    text-align: center;
}
.cart-item-checkbox .wx-checkbox-input{
	display: none;
}
.my-checkbox{
      display: inline-block;
  float: left;
  /* margin: 25rpx 20rpx 0 50rpx; */
  width: 40rpx;
  height: 40rpx;
  background-image: url("data:image/png;base64,");
  background-size: 100%;
}
.my-checkbox[checked] {
  background-image: url('data:image/png;base64,');
  background-size: 100%;
}
.cart-item-checkbox{
    position: absolute;
    z-index: 99;
    /* width: 40rpx;
    height: 40rpx; */
    left: 40rpx;
    top: 34rpx;
    display: flex;
    flex-flow: row nowrap;
    align-items: center;
    justify-content: center;

}
