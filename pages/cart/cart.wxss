.cart-btn{
    width: 690rpx;
    height: 92rpx;
    border-radius: 46rpx;
    background-color: #fff;
    position: fixed;
    bottom: 28rpx;
    left: 30rpx;
    z-index: 100;
    display: flex;
    flex-flow: row nowrap;
    justify-content: space-between;
    align-items: center;
}
.cart-btn-left{
    height: 100%;
    margin-left: 24rpx;
    display: flex;
    flex-flow: row nowrap;
     align-items: center;
     /* padding-top: 40rpx; */

}
.cart-btn-right{
    height: 100%;
    margin-right: 24rpx;
}
.bottom-checkbox{
    /* vertical-align: bottom; */
}
.cart-btn-left-choose{
    font-family: PingFangSC-Medium;
    font-size: 28rpx;
    color: #000000;
    font-weight: bold;
    margin-left: 10rpx;
     /* vertical-align: bottom; */
}
.bottom-checkbox .wx-checkbox-input{
	display: none;
}
.ws-checkbox {
  display: inline-block;
  float: left;
  /* margin: 25rpx 20rpx 0 50rpx; */
  width: 40rpx;
  height: 40rpx;
  background-image: url("data:image/png;base64,");
  background-size: 100%;
}

.ws-checkbox[checked] {
  background-image: url('data:image/png;base64,');
  background-size: 100%;
}
.cart-btn-right{
    display: flex;
      flex-flow: row nowrap;
     align-items: center;
     font-weight: bold;
}
.price-total{
    font-family: PingFangSC-Medium;
    font-size: 28rpx;
    color: #000000;
}
.red{
    color: #D53333;
}
.go-to-pay{
    font-family: PingFangSC-Medium;
    font-size: 32rpx;
    color: #FFFFFF;
    width: 124rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    background: #D53333;
    margin-left: 20rpx;
    border-radius: 30rpx;
}
.goods-delete{
    font-family: PingFangSC-Medium;
    font-size: 32rpx;
    color: #666666;
    width: 124rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    background: #FFFFFF;
    border: 1px solid #EEEEEE;
    margin-left: 20rpx;
    border-radius: 30rpx;
}