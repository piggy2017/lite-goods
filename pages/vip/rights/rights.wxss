page{
    min-height: 100%;
    /* background-color: #2E2928; */
}
.rights-page{
    width: 100%;
    height: 588rpx;
    padding-top: 460rpx;
}
.rights{
    width: 670rpx;
    height: 588rpx;
    background-color: #fff;
    border-radius: 12rpx;
    margin: 0 auto;
    position: relative;
}
.rights-title{
    width: 100%;
    height: 90rpx;
    /* opacity: 0.5; */
    background-image: linear-gradient(-180deg, #DCCFB8 0%, #FFFFFF 100%);
    border-radius: 12rpx 12rpx 0px 0px;
    border-bottom: 1rpx solid #ddd;
    display: flex;
    flex-flow: row nowrap;
    align-items: flex-end;
    justify-content: center;
}
.rights-title text{
    font-family: PingFangSC-Medium;
    font-size: 32rpx;
    color: #000000;
    font-weight: bold;
    margin-bottom: 10rpx;
}
.rights-icon{
    position: absolute;
    z-index: 99;
    top: -40rpx;
    left:248rpx;
    width: 174rpx;
    height: 68rpx;
}
.rights-icon image{
    width: 100%;
    height: 100%;
}
.rights-content{
    width: 100%;
    display: flex;
    flex-flow: row wrap;

}
.rights-content-item{
    width: 33.33%;
    display: flex;
    flex-flow: column nowrap;
    align-items: center;
    height: 246rpx;
    justify-content: center;
    position: relative;
}
.rights-content-item image{
    width: 84rpx;
    height: 84rpx;
}
.rights-content-item view{
    font-family: PingFangSC-Medium;
    font-size: 28rpx;
    color: #666666;
    margin-top: 20rpx;
}
.border-right::after{
    position: absolute;
    z-index: 99;
    right: 1rpx;
    top: 0;
    bottom: 0;
    width: 1rpx;
    background-color: #ddd;
    content: "";
}
.border-right::before{
    position: absolute;
    z-index: 99;
    left: 1rpx;
    top: 0;
    bottom: 0;
    width: 1rpx;
    background-color: #ddd;
    content: "";
}
.border-top{
     border-top: 1rpx solid #ddd;
}
.to-join{
    position: fixed;
    bottom: 20rpx;
    width: 670rpx;
    height: 84rpx;
}
.to-join image{
    width: 100%;
    height: 100%;
}


.rights-img1{
    width: 100%;
    height: 597rpx;
}
.rights-img1 image{
    width: 100%;
    height: 100%;
    display:block;
}
.rights-img2{
    width: 100%;
    height: 481rpx;
}
.rights-img2 image{
    width: 100%;
    display:block;
    height: 100%;
}
.rights-img3{
     width: 100%;
    height: 472rpx;
}
.rights-img3 image,.rights-img4 image,.rights-img5 image,.rights-img6 image{
    width: 100%;
    height: 100%;
    display:block;
}
.rights-img4{
    width: 100%;
    height: 613rpx;
}
.rights-img5{
    width: 100%;
    height: 599rpx;
}
.rights-img6{
    width: 100%;
    height: 351rpx;
    padding-bottom:90rpx;
}
.become-vip{
    position: fixed;
    z-index: 9;
    bottom: 0;
    left: 0;
    right: 0;
    width: 100%;
    height: 90rpx;
    background-image: url("data:image/jpeg;base64,");
    background-repeat: no-repeat;
    background-size: cover;
    font-family: PingFangSC-Medium;
    font-size: 32rpx;
    line-height: 90rpx;
    text-align: center;
    color: #C28F3D;
    letter-spacing: 0;
}