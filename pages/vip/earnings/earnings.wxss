.my-shouyi-top{
    background-image: url("data:image/png;base64,");
    height: 200rpx;
    width: 100%;
    background-size: 100%;
}
.yu-e{
    font-family: PingFangSC-Medium;
    font-size: 28rpx;
    color: #FFFFFF;
    padding:0 56rpx;
    margin-top: 32rpx;
}
.my-shouyi-top-edit{
    display: flex;
    flex-flow: row nowrap;
    justify-content: space-between;
    color: #FFF;
    padding: 0 56rpx;
    margin-top: 12rpx;
    align-items: center;
}
.my-shouyi-top-edit-yuan{
    font-size: 36rpx;
}
.my-shouyi-top-edit-yuan text{
    font-size: 60rpx;
}
.goTIxian{
    width: 160rpx;
    height: 56rpx;
    line-height: 56rpx;
    text-align: center;
    font-size: 28rpx;
    border-radius: 26rpx;
    background: #EA3939;
}
.shouyi-logs{
    position: absolute;
    z-index: 99;
    left: 0;
    right: 0;
    background-color: #fff;
    border-top-left-radius: 10rpx;
    border-top-right-radius: 10rpx;
    top: 180rpx;
}
.shouyi-logs-top{
    width: 710rpx;
    height: 52rpx;
    background: #EFEFEF;
    border-radius: 26rpx;
    margin: 20rpx 20rpx 0 20rpx;
    display: flex;
    flex-flow: row nowrap;
    justify-content: space-between;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #666666;
    line-height: 52rpx;
}
.shouyi-logs-top text:first-child{
    margin-left: 24rpx;
}
.shouyi-logs-top text:last-child{
    margin-right: 24rpx;
}
.shouyi-logs-item{
     width: 700rpx;
     padding: 0 5rpx;
     margin: 20rpx 20rpx 0 20rpx;
     font-family: PingFangSC-Medium;
    font-size: 28rpx;
     display: flex;
    flex-flow: row nowrap;
    justify-content: space-between;
    color: #333333;
}
.text-red{
    color: #D53333;
}
.haswidth{
    width: 250rpx;
}
.container-kong{
    margin: 240rpx auto 0 auto;
    width: 226rpx;
    height: 240rpx;
}
.container-kong image{
    width: 226rpx;
    height: 240rpx;
}
.coupon-none{
    text-align: center;
    margin-top: 30rpx;
    font-size: 30rpx;
    color: #f08d7e;
}
.shouyi-logs-item-2{
    color: #AAAAAA !important;
}
.shouyi-logs-item-2 .text-red{
     color: #AAAAAA !important;
}
.money{
  width:150rpx;text-align:right;
}
.dateWith{
  width: 290rpx;
}