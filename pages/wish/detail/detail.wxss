page{
    width: 100%;
    min-height: 100%;
    background-size: cover;
    background-repeat: no-repeat;
    font-family: PingFangSC-Regular;
    /* background-position: center center; */
    background-image: url("http://img.tracenet.cn/bg3.jpg")
}
.rules{
    width: 90rpx;
    height: 50rpx;
    text-align: center;
    line-height: 50rpx;
    background: #D51E1E;
    font-family: PingFangSC-Medium;
    font-size: 28rpx;
    color: #FFFFFF;
    letter-spacing: 1rpx;
    border-top-right-radius: 25rpx;
    border-bottom-right-radius: 25rpx;
    margin-top: 26rpx;
}
.detail-content{
    width: 702rpx;
    position: relative;
    background-color: #fff;
    margin-top: 26rpx;
    margin: 26rpx auto 10rpx auto;
    border-top-left-radius: 12rpx;
    border-top-right-radius: 12rpx;
    padding-bottom: 38rpx;
}
.user-name{
    font-family: PingFangSC-Medium;
    font-size: 30rpx;
    color: #333333;
    width: 100%;
    text-align: center;
    padding-top: 38rpx;
}

.user-pic{
    position: absolute;
    z-index: 9;
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
    border: 4rpx solid #fff;
    left: 310rpx;
    top: -50rpx;
}
.user-pic image{
    width: 100%;
    height: 100%;
    border-radius: 50%;
}
.fri-name{
   
}
.kanjia-status{
    width: 590rpx;
    height: 48rpx;
    background: #FFF7E6;
    border-radius: 24rpx;
    text-align: center;
    line-height: 48rpx;
    margin: 22rpx auto 30rpx auto;
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: #D08E20;
}
.goods-info{
    width: 642rpx;
    height: 132rpx;
    display: flex;
    flex-flow: row nowrap;
    justify-content: space-between;
    margin: 0 auto;
}
.goods-left{
    display: flex;
    flex-flow: row nowrap;
}
.goods-left image{
    width: 132rpx;
    height: 132rpx;
    border-radius: 8rpx;
}
.goods-center{
    width: 352rpx;
    margin-left: 20rpx;
    display: flex;
    flex-flow: column nowrap;
    justify-content: space-between;
}
.goods-name{
    font-family: PingFangSC-Medium;
    font-size: 28rpx;
    color: #333333;
    text-overflow:ellipsis;
    display:-webkit-box;
    -webkit-line-clamp:2;
    -webkit-box-orient:vertical !important;
    overflow:hidden;
    word-wrap:break-word;
    font-weight: bold;
}
.sale-num{
    font-size: 24rpx;
    color: #999999;
    margin-bottom: 10rpx;
}
.goods-right{
    font-size: 24rpx;
    color: #999999;
    text-align: right;
}
.before-price{
    font-family: PingFangSC-Semibold;
    font-size: 32rpx;
    color: #D53333;
    margin-bottom: 4rpx;
    font-weight: bold;
}
.time-content{
    font-family: PingFangSC-Medium;
    font-size: 28rpx;
    color: #333333;
    display: flex;
    flex-flow: row nowrap;
    justify-content: center;
}
.time-text{
    font-family: PingFangSC-Medium;
    font-size: 28rpx;
    color: #333333;
    font-weight: bold;
}
.time-text:first-child{
    margin-right: 14rpx;
}
.time-text:last-child{
    margin-left: 14rpx;
}
.time-list{
    width: 44rpx;
    height: 44rpx;
    text-align: center;
    line-height: 44rpx;
    background: #000000;
    border-radius: 4rpx;
    font-family: PingFangSC-Semibold;
    font-size: 30rpx;
    color: #FFFFFF;
}
.maohao{
    font-weight: bold;
    margin: 0 4rpx;
}
.hr{
    width: 642rpx;
    height: 6rpx;
    background-color: #FFB968;
    margin: 30rpx auto;
}
.has-reduce{
    width: 100%;
    text-align: center;
    margin-top: 10rpx;
    font-size: 24rpx;
    color: #AAAAAA;
}
.pro-content{
    width: 642rpx;
    margin: 0 auto;
    display: flex;
    flex-flow: row nowrap;
    justify-content: space-between;
}
.pro-content progress{
    width: 560rpx;
}
.red{
    color: #D53333;
}
.pro-content text{
    font-size: 24rpx;
    color: #AAAAAA;
}
.btn-content{
    display: flex;
    flex-flow: row nowrap;
    justify-content: center;
}
.btn{
    width: 280rpx;
    height: 72rpx;
    border-radius: 36rpx;
    text-align: center;
    line-height: 72rpx;
    font-family: PingFangSC-Medium;
    font-size: 30rpx;
    color: #FFFFFF;
    margin-top: 34rpx;
}
.btn-mySelf{
    background: #D53333;
    box-shadow: 0 10rpx 24rpx 6rpx rgba(156,7,7,0.22);
}
.btn-goumai{
    box-shadow: 0 10px 24px 6px rgba(185,103,4,0.27);
    background: #FDB234;
    margin-left: 30rpx;
}
.liji-buy{
    background: #D53333;
    box-shadow: 0 10rpx 24rpx 6rpx rgba(156,7,7,0.22);
    height: 72rpx;
    width: 590rpx;
    border-radius: 36rpx;
    text-align: center;
    line-height: 72rpx;
    font-family: PingFangSC-Medium;
    font-size: 30rpx;
    color: #FFFFFF;
    margin-top: 34rpx;
}
.userlist{
    width: 702rpx;
    padding: 30rpx 0;
    margin: 0 auto;
    background-color: #fff;
}
.title{
    font-size: 30rpx;
    color: #333333;
    display: flex;
    flex-flow: row nowrap;
}
.title view{
    width: 50%;
    position: relative;
    text-align: center;
}
.class-border::after{
    content: "";
    position: absolute;
    z-index: 9;
    width: 2rpx;
    height: 32rpx;
    background-color: #eee;
    right: 0;
}
.people-list{
    min-height: 200rpx;
    max-height: 300rpx;
    overflow: auto;
}
.user-item{
    display: flex;
    flex-flow: row nowrap;
    justify-content: space-between;
    font-size: 30rpx;
    color: #333333;
    align-items: center;
    margin-top: 30rpx;
}
.user-item-left{
    display: flex;
    flex-flow: row nowrap;

    align-items: center;
    width: 500rpx;
    
}
.user-item-right{
    width: 250rpx;
    
}
.user-item-left image{
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
    margin-right: 16rpx;
    margin-left: 100rpx;
}
.fri-name{
    width: 300rpx;
    text-overflow:ellipsis;
    display:-webkit-box;
    -webkit-line-clamp:1;
    -webkit-box-orient:vertical !important;
    overflow:hidden;
    word-wrap:break-word;
}
.kefu-content{
    width: 702rpx;
    margin: 10rpx auto 0 auto;
    padding: 30rpx 0;
    background-color: #fff;
    display: flex;
    flex-flow: row nowrap;
    justify-content: space-between;
    border-bottom-left-radius: 12rpx;
    border-bottom-right-radius: 12rpx;
}
.kefu-content-left{
    margin-left: 30rpx;
}
.kefu-content-right{
    background: #FFFFFF;
    border: 2rpx solid #D53333;
    border-radius: 15.5px;
    width: 200rpx;
    height: 62rpx;
    text-align: center;
    display: flex;
    flex-flow: row nowrap;
    justify-content: center;
    font-family: PingFangSC-Medium;
    font-size: 28rpx;
    color: #D53333;
    margin-right: 30rpx;
    align-items: center;
}
.kefu-content-right image{
    width: 40rpx;
    height: 40rpx;
}
.kefu-content-right text{
    margin-left: 12rpx;
}
.toast-content{
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background-color: rgba(0,0,0,0.7);
    z-index: 99;
    display: flex;
    flex-flow: row nowrap;
    align-items: center;
    justify-content: center;
}
.toast-wraper{
    width: 480rpx;
    height: 408rpx;
    background-color: #fff;
    position: relative;
}
.toast-image{
    width: 320rpx;
    height: 216rpx;
    position: absolute;
    z-index: 101;
    top: -170rpx;
    left: 80rpx;
}
.toast-image image{
    width: 320rpx;
    height: 216rpx;
}
.toast-text{
    font-size: 32rpx;
    text-align: center;
    color: #000000;
    margin-top: 100rpx;
}
.reduce-red{
    font-family: PingFangSC-Medium;
    font-size: 40rpx;
    color: #D53333;
    text-align: center;
    margin-top: 20rpx;
}
.hidden{
    display: none !important;
}
.close-model{
    background: #D53333;
    width: 346rpx;
    height: 72rpx;
    border-radius: 36rpx;
    font-family: PingFangSC-Medium;
    font-size: 30rpx;
    color: #FFFFFF;
    text-align: center;
    line-height: 72rpx;
    margin: 68rpx auto 0 auto;
}
.hidden{
    display: none !important;
}
.rule-mode{
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background-color: rgba(0,0,0,0.7);
    z-index: 99;
    display: flex;
    flex-flow: row nowrap;
    align-items: center;
    justify-content: center;
}
.rule-wraper{
    width: 600rpx;
    height: 630rpx;
    background: #FFFFFF;
    border-radius: 6px;
    position: relative;
}
.rules-content{
    width: 600rpx;
    height: 660rpx;
    background: #FFFFFF;
    border-radius: 6px;
    position: absolute;
    z-index: 101;
}
.rules-content-title{
    font-family: PingFangSC-Semibold;
    font-size: 32rpx;
    color: #333333;
    padding: 24rpx 0;
    font-weight: bold;
    text-align: center;
    border-bottom: 1rpx solid #eee;
}
.rules-content-bottom{
    padding: 29rpx 36rpx 36rpx 36rpx;
}
.rule-list{
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: #666666;
}
.rule-list:not(:first-child){
    margin-top: 20rpx;
}
.i-know{
    background: #D53333;
    box-shadow: 0 10rpx 18rpx 0 rgba(156,7,7,0.29);
    border-radius: 51rpx;
    font-family: PingFangSC-Medium;
    font-size: 32rpx;
    color: #FFFFFF;
    width: 400rpx;
    height: 72rpx;
    text-align: center;
    line-height: 72rpx;
    margin-top: 50rpx;
    margin-left: 64rpx;
}