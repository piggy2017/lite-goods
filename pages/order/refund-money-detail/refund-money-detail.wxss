page {
    width: 100%;
    min-height: 100%;
    background-color: #f2f2f2;
}

.refund-money {
    position: relative;
}

.order-detail-top {
    width: 100%;
    height: 200rpx;
    background-size: 100%;
    font-family: PingFangSC-Medium;
    font-size: 40rpx;
    color: #fff;
}

.reject-refund {
    margin-top: 20rpx;
    border-top: 1rpx solid #eee;
    padding-top: 20rpx;
}

.wait-audit {
    background-image: url("data:image/png;base64,");
}

.refund-seccess {
    background-image: url("data:image/png;base64,");
}

.refund-fail {
    background-image: url("data:image/png;base64,");
}

.order-detail-top .order-detail-top-wait-pay {
    margin-left: 30rpx;
}

.order-detail-top-wait-pay {
    padding-top: 44rpx;
}

.order-detail-time-next {
    opacity: 0.8;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #fff;
}

.order-detail-time {
    display: flex;
    flex-flow: row nowrap;
    margin-left: 30rpx;
    margin-top: 14rpx;
    font-size: 30rpx;
}

.order-detail-content {
    width: 710rpx;
    position: absolute;
    z-index: 9;
    top: 180rpx;
    left: 20rpx;
    padding-bottom: 140rpx;
}

.order-detail-info {
    padding: 0 20rpx 20rpx 20rpx;
    background-color: #fff;
    border-radius: 12rpx;
}

.order-detail-url {
    display: flex;
    flex-flow: row nowrap;
    justify-content: space-between;
    padding: 30rpx 0;
    position: relative;
    z-index: 1;
}

.order-detail-url::after {
    position: absolute;
    z-index: 99;
    bottom: 1rpx;
    left: 0rpx;
    right: 0rpx;
    height: 1rpx;
    background-color: #eee;
    content: "";
}

.order-detail-url-left {
    width: 132rpx;
    height: 132rpx;
    border-radius: 8rpx;
    margin-right: 18rpx;
}

.order-detail-url-left image {
    width: 132rpx;
    height: 132rpx;
    border-radius: 8rpx;
}

.order-detail-url-right {
    display: flex;
    flex-flow: row nowrap;
    justify-content: space-between;
}

.goods-detail-left {
    width: 400rpx;
    display: flex;
    flex-flow: column nowrap;
    justify-content: space-between;
}

.goods-detail-right {
    width: 118rpx;
    text-align: right;
    font-family: PingFangSC-Light;
    font-size: 24rpx;
    color: #888;
}

.goods-detail-right-price {
    font-size: 28rpx;
    color: #333;
    font-weight: bold;
}

.mar-top-10 {
    margin-top: 10rpx;
}

.my-post-imglist {
    display: flex;
    flex-flow: row nowrap;
    margin-bottom: 20rpx;
}

.my-post-imglist image {
    width: 132rpx;
    height: 132rpx;
}

.my-post-imglist image:not(:first-child) {
    margin-left: 30rpx;
}

.goods-detail-name {
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical !important;
    overflow: hidden;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #333;
}

.goods-detail-si {
    font-size: 24rpx;
    color: #999;
    margin-bottom: 10rpx;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical !important;
    overflow: hidden;
}

.order-cell {
    font-family: PingFangSC-Semibold;
    font-size: 28rpx;
    color: #333;
    display: flex;
    flex-flow: row nowrap;
    margin-bottom: 12rpx;
}

.order-cell-left {
    font-weight: bold;
}

.mar-top-12 {
    margin-top: 12rpx;
}

.text-red {
    color: #d53333;
}

.my-post {
    padding: 20rpx 20rpx 20rpx 20rpx;
    background-color: #fff;
    margin-top: 20rpx;
    border-radius: 12rpx;
}

.post-refund-reason {
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #333;
}

.post-refund-reason-label {
    font-weight: bold;
}

.call-kefu {
    height: 90rpx;
    width: 710rpx;
    margin-top: 20rpx;
    background: #fff;
    box-shadow: 0 8rpx 16rpx 0 rgba(0, 0, 0, 0.08);
    border-radius: 12rpx;
    font-family: PingFangSC-Regular;
    font-size: 32rpx;
    color: #000;
    display: flex;
    flex-flow: row nowrap;
    justify-content: center;
    align-items: center;
}

.call-kefu image {
    width: 30rpx;
    height: 30rpx;
    margin-right: 12rpx;
}

.post-range {
    padding: 30rpx 20rpx;
    margin-top: 20rpx;
    border-radius: 12rpx;
    width: 670rpx;
    height: 124rpx;
    background-color: #fff;
}

.post-range image {
    width: 720rpx;
    height: 124rpx;
}

.scroll-view_hot {
    white-space: nowrap;
    width: 670rpx;
    margin: 0 auto;
}

/* .step-wait-audit{
    background-image: url("")
} */

.refund-argin {
    width: 750rpx;
    height: 90rpx;
    background: #d53333;
    font-family: PingFangSC-Medium;
    font-size: 32rpx;
    text-align: center;
    line-height: 90rpx;
    color: #fff;
    position: fixed;
    bottom: 0;
    z-index: 99;
}
