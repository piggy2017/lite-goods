page {
    min-height: 100%;
    background-color: #f2f2f2;
}

.order-detail-top {
    width: 100%;
    height: 200rpx;
    background-size: 100%;
    font-family: PingFangSC-Medium;
    font-size: 40rpx;
    color: #fff;
}

.wait-pay {
    background-image: url("data:image/png;base64,");
}

.order-close {
    background-image: url("data:image/png;base64,");
}

.wait-fahuo {
    background-image: url("data:image/png;base64,");
}

.wait-shouhuo {
    background-image: url("data:image/png;base64,");
}

.wait-shouhuo {
    background-image: url("data:image/png;base64,");
}

.wait-comment {
    background-image: url("data:image/png;base64,");
}

.has-done {
    background-image: url("data:image/png;base64,");
}

.order-detail-top .order-detail-top-wait-pay {
    margin-left: 30rpx;
}

.order-detail-top-wait-pay {
    padding-top: 44rpx;
}

.order-detail-time-next {
    opacity: 0.8;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #fff;
}

.order-detail-time {
    display: flex;
    flex-flow: row nowrap;
    margin-left: 30rpx;
    margin-top: 14rpx;
    font-size: 30rpx;
}

.pad-time {
    height: 42rpx;
    width: 46rpx;
    line-height: 46rpx;
    text-align: center;
    /* opacity: 0.5; */
    background: rgba(255, 255, 255, 0.3);
    border-radius: 8rpx;
    color: #fff;
}

.hr {
    margin: 0 10rpx;
}

.order-detail {
    position: relative;
}

.order-detail-content {
    width: 710rpx;
    position: absolute;
    z-index: 9;
    top: 180rpx;
    left: 20rpx;
    padding-bottom: 140rpx;
}

.order-detail-address {
    min-height: 94rpx;
    background-color: #fff;
    border-radius: 12rpx;
    padding: 24rpx 20rpx 0 20rpx;
}

.order-detail-address-add {
    display: flex;
    flex-flow: row nowrap;
    padding-bottom: 30rpx;
}

.order-express {
    display: flex;
    flex-flow: row nowrap;
    padding: 29rpx 0;
    border-top: 1rpx solid #eee;
    background-image: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-size: 14rpx 24rpx;
    background-position: right 44rpx;
}

.order-express-left image {
    width: 68rpx;
    height: 68rpx;
}

.order-express-right {
    font-size: 28rpx;
    color: #333;
    margin-left: 24rpx;
    padding-right: 50rpx;
}

.order-express-new {
    font-family: PingFangSC-Medium;
    font-size: 32rpx;
    color: #333;
    font-weight: bold;
    width: 540rpx;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical !important;
    overflow: hidden;
    margin-bottom: 10rpx;
}

.order-detail-address-left image {
    width: 68rpx;
    height: 68rpx;
}

.order-detail-address-right {
    font-family: PingFangSC-Medium;
    font-size: 32rpx;
    color: #333;
    margin-left: 24rpx;
}

.order-detail-address-right-top {
    font-weight: bold;
}

.margin-left-30 {
    margin-left: 30rpx;
}

.add-detail {
    font-size: 28rpx;
    margin-top: 10rpx;
}

.order-detail-info {
    padding: 0 20rpx 20rpx 20rpx;
    background-color: #fff;
    margin-top: 20rpx;
    border-radius: 12rpx;
}

.goods-detail-name {
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical !important;
    overflow: hidden;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #333;
}

.goods-detail-si {
    font-size: 24rpx;
    color: #999;
    margin-bottom:10rpx;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical !important;
    overflow: hidden;
}

.order-detail-url {
    display: flex;
    flex-flow: row nowrap;
    justify-content: space-between;
    padding: 30rpx 0;
    position: relative;
    z-index: 1;
}

.order-detail-url::after {
    position: absolute;
    z-index: 99;
    bottom: 1rpx;
    left: 0rpx;
    right: 0rpx;
    height: 1rpx;
    background-color: #eee;
    content: "";
}

.order-detail-url-left {
    width: 132rpx;
    height: 132rpx;
    border-radius: 8rpx;
    margin-right: 18rpx;
}

.order-detail-url-left image {
    width: 132rpx;
    height: 132rpx;
    border-radius: 8rpx;
}

.order-detail-url-right {
    display: flex;
    flex-flow: row nowrap;
    justify-content: space-between;
}

.goods-detail-left {
    width: 400rpx;
    display: flex;
    flex-flow: column nowrap;
    justify-content: space-between;
}

.goods-detail-right {
    width: 118rpx;
    text-align: right;
    font-family: PingFangSC-Light;
    font-size: 24rpx;
    color: #888;
}

.goods-detail-right-price {
    font-size: 28rpx;
    color: #333;
    font-weight: bold;
}

.mar-top-10 {
    margin-top: 10rpx;
}

.order-cell {
    font-family: PingFangSC-Semibold;
    font-size: 28rpx;
    color: #333;
    font-weight: bold;
    display: flex;
    flex-flow: row nowrap;
    justify-content: space-between;
    margin-bottom: 12rpx;
}

.mar-top-12 {
    margin-top: 12rpx;
}

.text-red {
    color: #d53333;
}

.order-info-q {
    min-height: 90rpx;
    padding: 30rpx 20rpx;
    width: 670rpx;
    background-color: #fff;
    border-radius: 12rpx;
    margin-top: 20rpx;
}

.order-info-q-cell {
    display: flex;
    flex-flow: row nowrap;
    margin-bottom: 12rpx;
    font-size: 14px;
    color: #333;
}

.order-info-q-cell-left {
    font-family: PingFangSC-Semibold;
    font-size: 28rpx;
    color: #333;
    font-weight: bold;
}

.call-kefu {
    height: 90rpx;
    width: 710rpx;
    margin-top: 20rpx;
    background: #fff;
    box-shadow: 0 8rpx 16rpx 0 rgba(0, 0, 0, 0.08);
    border-radius: 12rpx;
    font-family: PingFangSC-Regular;
    font-size: 32rpx;
    color: #000;
    display: flex;
    flex-flow: row nowrap;
    justify-content: center;
    align-items: center;
}

.call-kefu image {
    width: 30rpx;
    height: 30rpx;
    margin-right: 12rpx;
}

.order-detail-bottomBtn {
    background: #fff;
    box-shadow: inset 0 2rpx 2rpx 0 rgba(255, 255, 255, 0.50);
    width: 100%;
    height: 96rpx;
    position: fixed;
    bottom: 0;
    z-index: 999;
    display: flex;
    flex-flow: row nowrap;
    justify-content: space-between;
    align-items: center;
}

.order-detail-bottomBtn-left {
    margin-left: 30rpx;
    font-family: PingFangSC-Semibold;
    font-size: 36rpx;
    color: #d53333;
    font-weight: bold;
}

.order-detail-bottomBtn-right {
    margin-right: 30rpx;
    display: flex;
    flex-flow: row nowrap;
    justify-content: flex-end;
}

.order-detail-bottom-btn {
    font-family: PingFangSC-Regular;
    font-size: 30rpx;
    width: 166rpx;
    height: 68rpx;
    border-radius: 4rpx;
    box-sizing: border-box;
    line-height: 68rpx;
    text-align: center;
}

.cancel-order-btn {
    background-color: #fff;
    color: #d53333;
    border: 1rpx solid #d53333;
}

.mrigin-right-20 {
    margin-right: 20rpx;
}

.sure-to-pay {
    color: #fff;
    background-color: #d53333;
}

.tuikuan-btn {
    position: absolute;
    z-index: 999;
    width: 136rpx;
    height: 44rpx;
    border: 1rpx solid #eee;
    box-sizing: border-box;
    line-height: 40rpx;
    text-align: center;
    background-color: #fff;
    right: 0;
    bottom: 28rpx;
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: #999;
    letter-spacing: 0.92rpx;
    border-radius: 4rpx;
}
