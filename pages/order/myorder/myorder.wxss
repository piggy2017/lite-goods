page{
    min-height: 100%;
    background-color: #f2f2f2;
     font-family: PingFangSC-Regular;
}
.container-kong{
    margin: 320rpx auto 0 auto;
    width: 288rpx;
    height: 240rpx;
}
.coupon-none{
    text-align: center;
    margin-top: 30rpx;
    font-size: 30rpx;
    color: #f08d7e;
}
.container-kong image{
    width: 288rpx;
    height: 240rpx;
}
.order-class{
    height: 84rpx;
    width: 100%;
    background-color: #fff;
    display: flex;
    flex-flow: row nowrap;
    align-items: center;
    border-top: 4rpx solid #f2f2f2;
    position: fixed;
    z-index: 99;
    top: 0;
}
.order-share-type{
    width:136rpx !important;
    height:52rpx !important;
    padding: 0 !important;
    overflow: hidden;
    position: absolute;
    z-index: 99;
    right: 0;
    top: 0;
}
.hr{
    width: 100%;
    height: 86rpx;
    background-color: #f2f2f2;
}
.order-class-item{
    width: 16.66%;
    height: 100%;
    text-align: center;
    font-size: 28rpx;
    color: #999999;
    line-height: 84rpx;
    position: relative;
}
.order-class-active{
    font-family: PingFangSC-Semibold;
    font-size: 36rpx;
    color: #000000;
    font-weight: bold;
}
.order-class-active::after{
    content: "";
    position: absolute;
    z-index: 99;
    bottom: 1rpx;
    left: 30rpx;
    right: 30rpx;
    height: 6rpx;
    background: #D53333;
}

.my-order-item{
    width: 100%;
    min-height: 330rpx;
    background-color: #fff;
    margin-top: 20rpx;
    /* padding-bottom: 20rpx; */
}

/* .my-order-item:first-child{
    margin-top: 120rpx !important;
} */
.my-order-item-title{
    height: 80rpx;
    display: flex;
    flex-flow: row nowrap;
    justify-content: space-between;
    align-items: center;
    font-family: PingFangSC-Semibold;
    font-size: 28rpx;
    color: #BBBBBB;
    padding: 0 30rpx;
}
.my-order-item-title-left{
    height: 100%;
    display: flex;
    flex-flow: row nowrap;
    align-items: center;
}
.my-order-item-title-left image{
    width: 32rpx;
    height: 32rpx;
    margin-right: 16rpx;
}
.my-order-item-title-right{
    color: #666;
    font-weight: bold;
}
.title-red{
    color: #D53333 !important;
}
.my-order-item-content-url{
    height: 132rpx;
    background: #F6F6F6;
    display: flex;
    flex-flow: row nowrap;
    justify-content: space-between;
    padding: 20rpx 30rpx;
}
.my-order-item-content-left{
    width: 132rpx;
    height: 132rpx;
    margin-right: 20rpx;
    border-radius: 8rpx;
}
.my-order-item-content-left image{
    width: 100%;
    height: 100%;
     border-radius: 8rpx;
}
.my-order-item-content-right{
    display: flex;
    flex-flow: row nowrap;
    justify-content: space-between;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #333333;
}
.my-order-goods-left{
    width: 420rpx;
    display:flex;
    flex-flow:column nowrap;
    justify-content:space-between;

}
.my-order-goods-name{
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #333333;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical !important;
     overflow: hidden;
}
.my-order-goods-speci{
    font-size: 24rpx;
    color: #999999;
    margin-bottom: 10rpx;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical !important;
    overflow: hidden;
}
.my-order-goods-right{
    width: 118rpx;
    text-align: right;
}
.unit-price{
    font-family: PingFangSC-Semibold;
    font-size: 28rpx;
    color: #333333;
}
.goods-num{
    font-family: PingFangSC-Light;
    font-size: 24rpx;
    color: #888888;
    margin-top: 8rpx;
}
.my-order-item-bot{
    text-align: right;
    padding-right: 30rpx;
}
.total-price{
    margin-top: 14rpx;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #666666;
}
.total-price-right{
    font-family: PingFangSC-Semibold;
    font-size: 28rpx;
    color: #333333;
    margin-left: 8rpx;
    font-weight: bold;
}
.my-order-item-btn{
    display: flex;
    flex-flow: row nowrap;
    justify-content: flex-end;
    margin-top: 20rpx;
    padding-bottom: 20rpx;
}
.my-order-item-btn-1{
    /* display: flex;
    flex-flow: row nowrap;
    justify-content: space-between; */
    position: relative;
    margin-top: 20rpx;
}
.order-btn{
    width: 136rpx;
    height: 52rpx;
    line-height: 52rpx;
    box-sizing: border-box;
    text-align: center;
    border-radius: 4rpx;
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    letter-spacing: 0.92rpx;
}
.cancel-order{
    background: #FFFFFF;
    border: 1rpx solid #D53333;
    color: #D53333;
}
.margin-right-20{
    margin-right: 20rpx;
}
.go-pay{
    background-color: #D53333;
    color: #fff;
}